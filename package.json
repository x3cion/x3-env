{
  "name": "x3-env",
  "version": "0.1.0",
  "description": "A env file reader written in Typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/x3cion/x3-env.git"
  },
  "author": "x3cion",
  "main": "dist/index.js",
  "scripts": {
    "test": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} nyc mocha -r ts-node/register -r source-map-support/register --recursive src/**/*.spec.ts",
    "prepublishOnly": "rm -rf dist/ && tsc && npm run test",
    "watch": "tsc -w"
  },
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.x",
    "@types/mocha": "5.x",
    "@types/node": "10.x",
    "chai": "4.x",
    "cross-env": "5.x",
    "mocha": "5.x",
    "nyc": "11.x",
    "source-map-support": "0.x",
    "ts-node": "6.x",
    "tslint": "5.x",
    "typescript": "2.x"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "include": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {}
}
